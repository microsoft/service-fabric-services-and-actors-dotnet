<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear /> <!--Adding this to avoid NuGet Security Analysis failure -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <!--Enable pre-release nuget feeds when needed to consume the private sf runtime bits
    <add key="myget.org_sf_prerelease" value="https://www.myget.org/F/sf-prerelease/api/v3/index.json" />
    <add key="ServiceFabric_NugetPackages_LocalPath" value="refs" /> -->
  </packageSources>
  <config>
    <!--
        Used to specify the default location to expand packages.
        See: NuGet.exe help install
        See: NuGet.exe help update
    -->
    <add key="repositoryPath" value="nuget\restored_packages" />
  </config>
</configuration>
