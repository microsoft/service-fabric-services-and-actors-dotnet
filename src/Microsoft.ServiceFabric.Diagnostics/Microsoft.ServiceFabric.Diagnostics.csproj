<Project Sdk="Microsoft.NET.Sdk">
  <Import Condition="'$(TargetFramework)' == 'netstandard2.0'" Project="..\..\properties\service_fabric_managed_netstandard.props" />
  <Import Condition="'$(TargetFramework)' == 'net462'" Project="..\..\properties\service_fabric_managed_netframework.props" />
  <Import Condition="'$(TargetFramework)' == 'net8.0'" Project="..\..\properties\service_fabric_managed_netcore.props" />

  <PropertyGroup>
      <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
      <AssemblyName>Microsoft.ServiceFabric.Diagnostics</AssemblyName>
      <TargetFrameworks>netstandard2.0;net462;net8.0</TargetFrameworks>
      <AppendTargetFrameworkToOutputPath>true</AppendTargetFrameworkToOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.ServiceFabric" />
  </ItemGroup>
</Project>